{
	"name": "@mirohq/cloud-data-import",
	"version": "0.4.0",
	"main": "dist/index.js",
	"bin": {
		"cloud-data-import": "./dist/cli.js"
	},
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/esm/index.js"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=20.x"
	},
	"scripts": {
		"start": "node -r tsconfig-paths/register dist/cli.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write \"**/*.{js,ts,json,md}\"",
		"prepare": "husky",
		"build:cjs": "tsc -p tsconfig.build.json && tsc-alias",
		"build:esm": "tsc -p tsconfig.esm.json && tsc-alias",
		"build": "npm run build:cjs && npm run build:esm",
		"release": "npm run build && changeset publish",
		"dev:setup": "npm install && npm run build && npm link",
		"dev": "ts-node -r tsconfig-paths/register src/cli.ts",
		"changeset": "changeset"
	},
	"license": "MIT",
	"devDependencies": {
		"@changesets/cli": "^2.27.7",
		"@types/inquirer": "^9.0.7",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.14.12",
		"@types/spinnies": "^0.5.3",
		"@types/yargs": "^17.0.32",
		"eslint": "^9.7.0",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.7",
		"prettier": "^3.3.3",
		"ts-jest": "^29.1.5",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@aws-sdk/client-auto-scaling": "^3.616.0",
		"@aws-sdk/client-athena": "^3.616.0",
		"@aws-sdk/client-cloudfront": "^3.614.0",
		"@aws-sdk/client-cloudtrail": "^3.556.0",
		"@aws-sdk/client-cloudwatch": "^3.614.0",
		"@aws-sdk/client-dynamodb": "^3.614.0",
		"@aws-sdk/client-ec2": "^3.616.0",
		"@aws-sdk/client-ecs": "^3.614.0",
		"@aws-sdk/client-efs": "^3.614.0",
		"@aws-sdk/client-eks": "^3.614.0",
		"@aws-sdk/client-elastic-load-balancing": "^3.609.0",
		"@aws-sdk/client-elastic-load-balancing-v2": "^3.614.0",
		"@aws-sdk/client-elasticache": "^3.616.0",
		"@aws-sdk/client-lambda": "^3.614.0",
		"@aws-sdk/client-rds": "^3.620.0",
		"@aws-sdk/client-redshift": "3.614.0",
		"@aws-sdk/client-route-53": "^3.600.0",
		"@aws-sdk/client-s3": "^3.614.0",
		"@aws-sdk/client-sns": "^3.616.0",
		"@aws-sdk/client-sqs": "^3.614.0",
		"@aws-sdk/client-sts": "^3.614.0",
		"@aws-sdk/smithy-client": "^3.374.0",
		"@aws-sdk/util-arn-parser": "^3.568.0",
		"@mirohq/prettier-config": "^2.0.0",
		"colorette": "^2.0.20",
		"dotenv": "^16.4.5",
		"inquirer": "^8.2.6",
		"spinnies": "^0.5.1",
		"yargs": "^17.7.2"
	},
	"lint-staged": {
		"*.ts": [
			"npm test -- --findRelatedTests --passWithNoTests",
			"npm run lint:fix",
			"npm run format"
		]
	}
}